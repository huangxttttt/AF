{
  "manifest_version": 3,
  "name": "暗访者",
  "version": "1.0",
  "description": "Click toolbar icon to show random text and send body HTML to backend.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": [
    "https://your-api.example.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
	  "128": "icon.png"
	},
  "action": {
	  "default_icon": {
		"128": "icon.png"
	  }
  }
}
